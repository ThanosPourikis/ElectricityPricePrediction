{% extends "base.jinja" %}
{% block content %}
	<h1>{{title}}</h1>
	<div id="chart_json"></div>
	<script>
		var graph = {{ chart_json | safe }};
		Plotly.plot('chart_json', graph, {});
	</script>
	<table class="table">
		<thead>
			<tr>
				<th>Train error</th>
				<th>Validation error</th>
				<th>Test error</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td scope="row">{{train_error}}</td>
				<td>{{validate_error}}</td>
				<td>{{test_error}}</td>
			</tr>
		</tbody>
	</table>
	<h3></h3>
	<br>
	<h3></h3>
	<br>
	<h3></h3>
	{{ model }}
	{% block more_data %}
	{% endblock more_data %}
{% endblock content %}
